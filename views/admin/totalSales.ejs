<%- include('../layout/adminHeader') %>
        <section class="content-main">
            <div class="content-header">
                <div>
                    <h2 class="content-title card-title">Total Sales Report</h2>
                </div>
                <div>
                    <h6>Download Report</h6>
                    <a href="/admin/download-total-sales-report" class="btn " style="background-color: #088178; color: white;">PDF</a>
                    <a href="/admin/download-total-sales-report-excel" class="btn" style="background-color: #088178; color: white;">Excel</a>
                </div>
                
            </div>
            <div class="card mb-4">
             <!-- card-header end// -->
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="myTable" >
                            <thead>
                                <tr>
                                    <th scope="col">OrderId</th>
                                    <th scope="col">Date</th>
                                    <th scope="col">User</th>
                                    <th scope="col">Method</th>
                                    <th scope="col">Total</th>
                                </tr>
                            </thead>
                            <tbody >
                                <% if(typeof orders !== 'undefined'){ %>

                                <% for(let i=0;i<orders.length;i++){ %>

                                
                                <tr>
                                    <td><%= orders[i]._id %></td>
                                    <td><%= orders[i].date %></td>
                                    <td><template class="__cf_email__" data-cfemail="aec3cfdcd8c7c0eecbd6cfc3dec2cb80cdc1c3">[email&#160;protected]</template><%=orders[i].userId.name %></td>
                                    <td><%= orders[i].paymentMethod %></td>
                                    <td><%= orders[i].orderValue %></td>
                                </tr>
                                    <% } %>
                            <% } %>
                            </tbody>
                        </table>
                    </div> <!-- table-responsive //end -->
                </div> <!-- card-body end// -->
            </div> <!-- card end// -->
        </section> <!-- content-main end// -->
        <footer class="main-footer font-xs">
            <div class="row pb-30 pt-15">
                <div class="col-sm-6">
                    <script data-cfasync="false" src="../../../cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
                    document.write(new Date().getFullYear())
                    </script>
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-end">
                        All rights reserved
                    </div>
                </div>
            </div>
        </footer>
    </main>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="assets/js/vendors/jquery-3.6.0.min.js"></script>
    <script src="assets/js/vendors/bootstrap.bundle.min.js"></script>
    <script src="assets/js/vendors/select2.min.js"></script>
    <script src="assets/js/vendors/perfect-scrollbar.js"></script>
    <script src="assets/js/vendors/jquery.fullscreen.min.js"></script>
    <!-- Main Script -->
    <script src="assets/js/main.js" type="text/javascript"></script>

