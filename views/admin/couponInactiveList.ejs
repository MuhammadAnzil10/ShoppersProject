<%- include('../layout/adminHeader') %>
 
<section class="content-main">
    <div class="content-header">
        <div>
            <h2 class="content-title card-title">Inactive Coupons </h2>
            <p>Whole data about your business here</p>
        </div>
       <div>
        </div> 
    </div>
        <div class="card mb-4">
          <!-- card-header end// -->
             <div class="card-body">
                 <div class="table-responsive">
                     <table  id="myTable" >
                         <thead>
                             <tr>
                                 <th scope="col">Coupon Code</th>
                                 <th scope="col">Coupon Description:</th>
                                 <th scope="col" style="padding-left: 15px;">Discount Percentage</th>
                                 <th scope="col" style="padding-left: 21px;">Maximum Discount</th>
                                 <th scope="col" style="padding-left: 20px;">Minimum value</th>
                                 <th scope="col" style="padding-left: 21px;">Validity</th>
                                 <th scope="col" style="padding-left: 21px;">Usage Count</th>
                                 <th scope="col" style="padding-left: 21px;">Created on</th>
                                 <th scope="col" style="padding-left: 21px;">Status</th>
                                 <th scope="col" style="padding-left: 21px;">Action</th>
                              
                               

                             </tr>
                         </thead>
                        <tbody>
                            <% for(let i=0;i<coupons.length;i++){ %>


                           
                            <tr>
                             <td scope="col"><%= coupons[i].couponCode %> </td>
                                 <td scope="col"><%= coupons[i].couponDescription %></td>
                                 <td scope="col" style="padding-left: 15px;"><%= coupons[i].discountPercentage %></td>
                                 <td scope="col" style="padding-left: 21px;"><%= coupons[i].maxDiscountAmount %></td>
                                 <td scope="col" style="padding-left: 20px;"><%= coupons[i].minOrderValue %></td>
                                 <td scope="col" style="padding-left: 21px;"> <%= coupons[i].validFor %></td>
                                 <td scope="col" style="padding-left: 21px;"> <%= coupons[i].usageCount %></td>
                                 <td scope="col" style="padding-left: 21px;"> <%= coupons[i].createdOn.toLocaleString() %></td>
                                 <td scope="col" style=" width: 40px; color: red;">Inactive</td>
                                 <td scope="col" style=" width: 40px;"><a href="/admin/activate-coupon?id=<%=coupons[i]._id%>" class="btn btn-primary">Activate</a></td>

                           
                                
                                 
                                 
                                 
                                 
                       
                                </tr>
                                <% } %>
                         </tbody>
                     </table>
                 </div> <!-- table-responsive //end -->
             </div> <!-- card-body end// -->
         </div> <!-- card end// -->
</section>
  <!-- content-main end// -->
  <footer class="main-footer font-xs">
    <div class="row pb-30 pt-15">
        <div class="col-sm-6">
            <script>
            document.write(new Date().getFullYear())
            </script> Â©, Shoppers .
        </div>
        <div class="col-sm-6">
            <div class="text-sm-end">
                All rights reserved
            </div>
        </div>
    </div>
</footer>
</main>
<script src="assets/js/vendors/jquery-3.6.0.min.js"></script>
<script src="assets/js/vendors/bootstrap.bundle.min.js"></script>
<script src="assets/js/vendors/select2.min.js"></script>
<script src="assets/js/vendors/perfect-scrollbar.js"></script>
<script src="assets/js/vendors/jquery.fullscreen.min.js"></script>
<script src="assets/js/vendors/chart.js"></script>
<!-- Main Script -->
<script src="assets/js/main.js" type="text/javascript"></script>
<script src="assets/js/custom-chart.js" type="text/javascript"></script>
<script type="text/javascript" charset="utf8" src="http://ajax.aspnetcdn.com/ajax/jquery.dataTables/1.9.0/jquery.dataTables.min.js"></script>

<script>

$(document).ready( function () {
    
    $('#myTable').DataTable();
} );

</script>
</body>


